{
  "name": "kokk",
  "version": "1.0.0",
  "description": "A beautiful way to create a documentation from markdown.",
  "repository": {
    "url": "luyilin/kokk",
    "type": "git"
  },
  "main": "dist/kokk.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext .js --ext .vue",
    "test": "npm run lint && npm run build",
    "build": "poi build --component Kokk",
    "example": "poi -c docs/dev/poi.config.js"
  },
  "author": {
    "name": "luyilin",
    "email": "luyilin12@gmail.com"
  },
  "license": "MIT",
  "eslintConfig": {
    "extends": [
      "plugin:vue/recommended"
    ],
    "rules": {
      "vue/max-attributes-per-line": [
        2,
        {
          "singleline": 2,
          "multiline": {
            "max": 2,
            "allowFirstLine": true
          }
        }
      ]
    }
  },
  "devDependencies": {
    "eslint": "^4.14.0",
    "eslint-plugin-vue": "^4.0.1",
    "poi": "^9.3.10",
    "rollup-plugin-vue": "^2.5.2",
    "vue-test-utils": "^1.0.0-beta.9"
  },
  "dependencies": {
    "jump.js": "^1.0.2",
    "marked3": "^0.5.2",
    "node-sass": "^4.7.2",
    "prismjs": "^1.12.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "slugo": "^0.2.1",
    "throttleit": "^1.0.0",
    "unfetch": "^3.0.0",
    "vue-cute-loading": "^0.0.2",
    "vue-cute-rate": "^0.0.6",
    "vue-juri": "^0.0.8",
    "vue-router": "^3.0.1",
    "vue-style-loader": "^4.0.2"
  }
}
